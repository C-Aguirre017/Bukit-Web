<p id="notice"><%= notice %></p>

<h1>Listing Applications</h1>

<table>
  <thead>
    <tr>
      <th>Pin</th>
      <th>User</th>
      <th>Reached</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @applications.each do |application| %>
      <tr>
        <td><%= application.pin %></td>
        <td><%= application.user %></td>
        <td><%= application.reached %></td>
        <td><%= link_to 'Show', application %></td></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

