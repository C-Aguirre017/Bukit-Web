<p id="notice"><%= notice %></p>

<h1>Listing Pins</h1>

<table>
  <thead>
    <tr>
      <th>User</th>
      <th>Publication</th>
      <th>Realization</th>
      <th>Duration</th>
      <th>Title</th>
      <th>Description</th>
      <th>Price</th>
      <th>Help type</th>
      <th>Faculty</th>
      <th>Latitude</th>
      <th>Longitude</th>
      <th>Course</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pins.each do |pin| %>
      <tr>
        <td><%= pin.user %></td>
        <td><%= pin.publication %></td>
        <td><%= pin.realization %></td>
        <td><%= pin.duration %></td>
        <td><%= pin.title %></td>
        <td><%= pin.description %></td>
        <td><%= pin.price %></td>
        <td><%= pin.help_type %></td>
        <td><%= pin.faculty %></td>
        <td><%= pin.latitude %></td>
        <td><%= pin.longitude %></td>
        <td><%= pin.course %></td>
        <td><%= link_to 'Show', pin %></td>
        <td><%= link_to 'Edit', edit_pin_path(pin) %></td>
        <td><%= link_to 'Destroy', pin, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pin', new_pin_path %>
